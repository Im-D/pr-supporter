{
  "name": "pr-supporter",
  "version": "1.0.0",
  "description": "pr-supporter",
  "author": "Im-D <jp302119@gmail.com> (https://github.com/Im-D)",
  "license": "ISC",
  "repository": "https://github.com//pr-supporter.git",
  "homepage": "https://github.com//pr-supporter",
  "bugs": "https://github.com//pr-supporter/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "dev": "nodemon",
    "start": "probot run ./index.js",
    "lint": "standard --fix",
    "test": "jest",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage",
    "precommit": "lint-staged && npm run test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "git add"
    ]
  },
  "dependencies": {
    "probot": "^7.2.0"
  },
  "devDependencies": {
    "husky": "^2.7.0",
    "jest": "^24.0.0",
    "lint-staged": "^8.2.1",
    "nock": "^10.0.0",
    "nodemon": "^1.17.2",
    "smee-client": "^1.0.2",
    "standard": "^12.0.1"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
